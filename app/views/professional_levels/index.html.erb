<p id="notice"><%= notice %></p>

<h1>Listing Professional Levels</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @professional_levels.each do |professional_level| %>
      <tr>
        <td><%= professional_level.name %></td>
        <td><%= link_to 'Show', professional_level %></td>
        <td><%= link_to 'Edit', edit_professional_level_path(professional_level) %></td>
        <td><%= link_to 'Destroy', professional_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Professional level', new_professional_level_path %>
